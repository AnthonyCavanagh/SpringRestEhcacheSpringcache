<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">

	<cache name="fundCachce" maxBytesLocalHeap="100M" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="120" memoryStoreEvictionPolicy="FIFO">
		<persistence strategy="none" />
	</cache>
	
	<cache name="accountCachce" maxBytesLocalHeap="100M" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="120" memoryStoreEvictionPolicy="FIFO">
		<persistence strategy="none" />
	</cache>
	
	<cache name="bankCachce" maxBytesLocalHeap="100M" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="120" memoryStoreEvictionPolicy="FIFO">
		<persistence strategy="none" />
	</cache>
</ehcache>     